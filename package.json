        "eslint-plugin-vue": "^10.6.2",
        "jiti": "^2.6.1",
        "jsdom": "^27.3.0",
        "npm-run-all2": "^8.0.4",
        "oxlint": "^1.32.0",
        "rollup-plugin-visualizer": "^6.0.5",
        "sass": "^1.95.0",
        "typescript": "~5.9.3",
        "vite": "^7.2.7",
        "vite-plugin-dts": "^4.5.4",
        "vite-plugin-vue-devtools": "^8.0.5",
