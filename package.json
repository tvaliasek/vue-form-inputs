            "import": "./dist/vue-form-inputs.css",
            "require": "./dist/vue-form-inputs.css"
        },
        "./package.json": "./package.json"
    },
    "types": "./dist/index.d.ts",
    "main": "dist/vue-form-inputs.umd.js",
    "module": "dist/vue-form-inputs.js",
    "homepage": "https://github.com/tvaliasek/vue-form-inputs#readme",
    "bugs": {
        "url": "https://github.com/tvaliasek/vue-form-inputs/issues"
    },
    "keywords": [
        "form",
        "form input",
        "bootstrap",
        "vue"
    ],
    "license": "MIT",
    "author": {
        "name": "Tomáš Valiašek",
        "url": "https://github.com/tvaliasek"
    },
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "build-only": "vite build",
